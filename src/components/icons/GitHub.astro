---
interface Props {
  class?: string;
}
const { class: className = 'w-5 h-5' } = Astro.props;
import githubDark from './github-mark.svg';
import githubLight from './github-mark-white.svg';
---

<img src={githubDark.src} alt="GitHub" class={`${className} dark:hidden`} />
<img src={githubLight.src} alt="GitHub" class={`${className} hidden dark:block`} />
